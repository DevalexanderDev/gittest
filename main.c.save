#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main(int argc, char ** argv)
	//setlocale(LC_ALL, "rus");
	vector <char> str;
	if(strlen(argv[1]) && strlen(argv[1]) > 10)
		return 1;
	for(int i = 0; i < strlen(argv[1]);i++){
		str.pushback(toupper(argv[1][i]));
		//printf("%c", toupper(argv[1][i]));
	}
	printf(str);
	printf("\n");


	return 0;
}
